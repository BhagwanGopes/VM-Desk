<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Core virtualization access -->
	<key>com.apple.security.virtualization</key>
	<true/>

	<!-- Network access (NAT mode) -->
	<key>com.apple.security.network.client</key>
	<true/>
	<key>com.apple.security.network.server</key>
	<true/>

	<!-- JIT compilation for VMs -->
	<key>com.apple.security.cs.allow-jit</key>
	<true/>

	<!-- Load VM plugins/frameworks -->
	<key>com.apple.security.cs.disable-library-validation</key>
	<true/>

	<!-- Restricted entitlements (may require Apple approval) -->
	<!-- Uncomment when ready to request DTS approval -->
	<!--
	<key>com.apple.vm.networking</key>
	<true/>
	<key>com.apple.vm.device-access</key>
	<true/>
	-->
</dict>
</plist>
